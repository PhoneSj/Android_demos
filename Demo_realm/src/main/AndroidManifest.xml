<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.phone.demo.realm">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.DogListActivity"/>
        <activity android:name=".activity.QueryActivity"/>
        <activity android:name=".activity.QueryAllActivity"/>
        <activity android:name=".activity.UpdateActivity"/>
        <activity android:name=".activity.QueryByAcitivity"/>
        <activity android:name=".activity.StatisticalActivity"/>
    </application>

</manifest>